{
    "name": "noccylabs/foundation-bundle",
    "description": "Quickly add bootstrap, jquery and more from CDNs",
    "license": "GPL-3.0",
    "keywords": [
        "bootstrap", "bootswatch", "jquery", "cdn"
    ],
    "authors": [
        {
            "name": "Christopher Vagnetoft",
            "email": "cvagnetoft@gmail.com"
        }
    ],
    "require": {
        "symfony/yaml": "^2.0|^3.0|^4.0",
        "symfony/dependency-injection": "^2.0|^3.0|^4.0",
        "psr/log": "^1.0|^2.0"
    },
    "require-dev": {
        "symfony/console": "^2.0|^3.0|^4.0",
        "symfony/finder": "^2.0|^3.0|^4.0"
    },
    "autoload": {
        "psr-0": { "NoccyLabs\\":"src/" }
    },
    "bin": [
        "bin/foundation"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
